{
  "name": "typedoc-packages-example",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "tsc --build",
    "docs": "typedoc --plugin typedoc-plugin-markdown",
    "docs-all": "npm run docs --workspaces --if-present"
  },
  "dependencies": {
    "typedoc": "^0.25.2",
    "typedoc-plugin-markdown": "^4.0.0-next.36",
    "typescript": "^5.0.4"
  }
}
